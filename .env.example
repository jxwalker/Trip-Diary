# Backend and Frontend configuration
# Backend FastAPI base URL (used by Next.js proxy server routes)
BACKEND_URL=http://localhost:8000

# Public API base URL (optional; for client-side fetches if used)
NEXT_PUBLIC_API_URL=http://localhost:8000

# LLM Providers (set at least one for extraction/parsing)
OPENAI_API_KEY=
ANTHROPIC_API_KEY=

# Perplexity (required for enhanced real-time guide)
PERPLEXITY_API_KEY=
PERPLEXITY_MODEL=sonar

# Maps and Places
GOOGLE_MAPS_API_KEY=

# Weather
OPENWEATHER_API_KEY=
